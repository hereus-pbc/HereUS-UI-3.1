        },
        ".body_pink nav.Topbar": {
            "background-color": "#200036"
        },
        ".the_box": {
            "background-color": "#15151580"
        },
        ".body_red .the_box": {
            "background-color": "#1B0000"
        },
        ".body_orange .the_box": {
            "background-color": "#1B1000"
        },
        ".body_yellow .the_box": {
            "background-color": "#1B1000"
        },
        ".body_green .the_box": {
            "background-color": "#001B00"
        },
        ".body_blue .the_box": {
            "background-color": "#00001B"
        },
        ".body_pink .the_box": {
            "background-color": "#10001B"
        },
        "nav.Navbar a.inactive button": {
            "color": "#FFFFFF"
        },
        "nav.Topbar a.inactive button": {
            "color": "#FFFFFF"
        },
        ".textbox": {
            "background-color": "#2A2A2A",
            "color": "#FFFFFF",
            "-webkit-appearance": "none"
        },
        ".textbox.grey": {
            "background-color": "#151515",
