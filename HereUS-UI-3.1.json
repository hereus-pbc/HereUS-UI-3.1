    "::-webkit-scrollbar": {
        "width": "0px"
    },
    ".feed": {
        "padding-right": "100px",
        "padding-left": "100px"
    },
    ".centerfeed": {
        "padding-right": "350px",
        "padding-left": "100px"
    },
    ".rightbar": {
        "position": "fixed",
        "top": "40px",
        "bottom": "0px",
        "right": "0px",
        "z-index": "1000",
        "display": "block",
        "padding": "20px",
        "padding-left": "0px",
        "overflow-x": "hidden",
        "overflow-y": "auto",
        "border-right": "none"
    },
    "#page-wrap": {
        "width": "1300px",
        "margin": "0 auto"
    },
    "textarea": {
        "margin": "3px",
        "font-family": [
            "Catamaran",
            "Arial",
            "Helvetica",
            "sans-serif"
        ]
    },
    "textarea:focus": {
        "outline": "none"
    }
