        ".body_yellow .textbox.grey": {
            "background-color": "#1B1000"
        },
        ".body_green .textbox.grey": {
            "background-color": "#001B00"
        },
        ".body_blue .textbox.grey": {
            "background-color": "#00001B"
        },
        ".body_pink .textbox.grey": {
            "background-color": "#10001B"
        },
        ".body_red .textbox": {
            "background-color": "#360000"
        },
        ".body_orange .textbox": {
            "background-color": "#362000"
        },
        ".body_yellow .textbox": {
            "background-color": "#363600"
        },
        ".body_green .textbox": {
            "background-color": "#003600"
        },
        ".body_blue .textbox": {
            "background-color": "#000036"
        },
        ".body_pink .textbox": {
            "background-color": "#200036"
        },
        "a": {
            "color": "inherit"
        },
        ".autocolor": {
            "color": "#FFFFFF"
        },
        ".body_red .app_icon": {
            "background-color": "#800000"
        },
        ".body_orange .app_icon": {
            "background-color": "#804C00"
        },
        ".body_yellow .app_icon": {
            "background-color": "#808000"
        },
        ".body_green .app_icon": {
            "background-color": "#004D00"
        },
        ".body_blue .app_icon": {
            "background-color": "#00004D"
        },
        ".body_pink .app_icon": {
            "background-color": "#004D00"
        }
    },
    ".body_red .autocolor.background": {
        "background-color": "#FF0000"
    },
    ".body_orange .autocolor.background": {
        "background-color": "#FFA200"
    },
    ".body_yellow .autocolor.background": {
        "background-color": "#FFFF00"
    },
    ".body_green .autocolor.background": {
        "background-color": "#00FF00"
    },
    ".body_blue .autocolor.background": {
        "background-color": "#007DD1"
    },
    ".body_pink .autocolor.background": {
