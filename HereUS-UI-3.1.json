{
    "@imports": [
        "https://fonts.googleapis.com/css2?family=Noto+Emoji:wght@700&display=swap",
        "https://fonts.googleapis.com/css2?family=Encode+Sans&display=swap",
        "https://fonts.googleapis.com/css2?family=Satisfy&display=swap",
        "https://fonts.googleapis.com/css2?family=Inter&display=swap",
        "https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200",
        "https://fonts.googleapis.com/css2?family=Parisienne&display=swap",
        "https://fonts.googleapis.com/css2?family=Croissant+One&family=Parisienne&display=swap"
    ],
    "data": {
        "visibility": "hidden",
        "height": "0px",
        "width": "0px",
        "font-size": "0px"
    },
    "p:hover": {
        "border": "none"
    },
    "body": {
        "padding-top": "0px",
        "font-family": [
            "Catamaran",
            "Arial",
            "'Noto Emoji'",
            "Helvetica",
            "sans-serif"
        ],
        "overflow-x": "hidden",
        "scroll-behavior": "smooth"
    },
    ".caution::after": {
        "content": "var(--caution-content)",
        "font-weight": "500",
        "font-size": "15px",
        "margin-left": "-30px",
        "background-color": "#FF0000",
        "z-index": "9999",
        "position": "relative",
        "padding-left": "4px",
        "padding-right": "4px",
        "border-radius": "10px",
        "margin-top": "-50px"
    },
    ".body_red": {
        "background-color": "#FFCECE",
        "accent-color": "#FF0000"
    },
    ".body_orange": {
        "background-color": "#FFF8E4",
        "accent-color": "#FFA200"
    },
    ".body_yellow": {
        "background-color": "#FFFFE6",
        "accent-color": "#FFFF00"
    },
    ".body_green": {
        "background-color": "#DFFFDF",
        "accent-color": "#00FF00"
    },
    ".body_blue": {
        "background-color": "#D8E7FF",
        "accent-color": "#007DD1"
    },
    ".body_pink": {
        "background-color": "#FFD7FA",
        "accent-color": "#FF00E1"
    },
    ".the_box": {
        "background-color": "#F0F0F0",
        "backdrop-filter": "blur(10px)",
        "border": "1px solid #808080A0",
        "border-radius": "10px",
        "padding": "20px",
        "padding-top": "0px",
        "padding-bottom": "10px"
    },
    ".body_red .the_box": {
        "background-color": "#FFE9E9"
    },
    ".body_orange .the_box": {
        "background-color": "#FFFAEC"
    },
    ".body_yellow .the_box": {
        "background-color": "#FFFF8E"
    },
    ".body_green .the_box": {
        "background-color": "#E8FFE8"
    },
    ".body_blue .the_box": {
        "background-color": "#E5EFFF"
    },
    ".body_pink .the_box": {
        "background-color": "#FFE6FC"
    },
    "nav.Navbar": {
        "position": "fixed",
        "height": "100%",
        "top": "20px",
        "bottom": "20px",
        "left": "10px",
        "width": "60px",
        "overflow": "hidden",
        "z-index": "9999",
        "padding-right": "5px",
        "border-radius": "10px"
    },
    "nav.Topbar": {
        "position": "fixed",
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px",
        "height": "75px",
        "overflow": "hidden",
        "z-index": "9999",
        "padding-right": "5px",
        "border-radius": "10px",
        "margin-top": "-10px"
    },
    ".body_red nav.Topbar": {
        "background-color": "#FFCECE"
    },
    ".body_orange nav.Topbar": {
        "background-color": "#FFF8E4"
    },
    ".body_yellow nav.Topbar": {
        "background-color": "#FFFFE6"
    },
    ".body_green nav.Topbar": {
        "background-color": "#DFFFDF"
    },
    ".body_blue nav.Topbar": {
        "background-color": "#D8E7FF"
    },
    ".body_pink nav.Topbar": {
        "background-color": "#FFD7FA"
    },
    "nav.Navbar a.active button span font": {
        "color": "#FFFFFF"
    },
    "nav.Navbar a.titleicon": {
        "float": "left",
        "text-align": "center",
        "text-decoration": "none",
        "padding-left": "10px",
        "filter": "drop-shadow(1px 1px 1px #0084FF)"
    },
    "nav.Navbar a.active button": {
        "background-color": "#00000080",
        "border-radius": "15px",
        "color": "#FFFFFF",
        "border": "none",
        "padding": "14px 16px",
        "align-items": "left",
        "cursor": "pointer"
    },
    "nav.Navbar a.inactive button": {
        "background-color": "#007DD100",
        "border-radius": "15px",
        "color": "#000000",
        "border": "none",
        "padding": "14px 16px",
        "align-items": "left",
        "cursor": "pointer"
    },
    "nav.Topbar a.inactive button": {
        "background-color": "#007DD100",
        "border-radius": "15px",
        "color": "#000000",
        "border": "none",
        "padding": "14px 16px",
        "align-items": "left",
        "cursor": "pointer",
        "float": "right",
        "filter": "drop-shadow(1px 1px 1px #808080A0)"
    },
    ".body_red nav.Navbar a.active button": {
        "background-color": "#FF000080"
    },
    ".body_orange nav.Navbar a.active button": {
        "background-color": "#FFA20080"
    },
    ".body_yellow nav.Navbar a.active button": {
        "background-color": "#FFFF0080"
    },
    ".body_green nav.Navbar a.active button": {
        "background-color": "#00FF0080"
    },
    ".body_blue nav.Navbar a.active button": {
        "background-color": "#007DD180"
    },
    ".body_pink nav.Navbar a.active button": {
        "background-color": "#FF00E180"
    },
    "nav.Navbar a": {
        "float": "right",
        "text-align": "center",
        "text-decoration": "none",
        "font-size": "17px",
        "color": "#FFFFFF",
        "align-items": "left"
    },
    ".button": {
        "border-radius": "15px",
        "border": "none",
        "background-color": "#007DD1",
        "color": "whitesmoke",
        "width": "400px",
        "height": "50px",
        "font-size": "20px",
        "cursor": "pointer"
    },
    ".button.small": {
        "border-radius": "15px",
        "width": "150px",
        "height": "50px",
        "font-size": "15px"
    },
    ".button.mini": {
        "border-radius": "15px",
        "width": "100px",
        "height": "30px",
        "font-size": "15px"
    },
    ".body_red .button": {
        "background-color": "#FF0000"
    },
    ".body_orange .button": {
        "background-color": "#FFA200"
    },
    ".body_yellow .button": {
        "background-color": "#FFFF00"
    },
    ".body_green .button": {
        "background-color": "#00FF00"
    },
    ".body_blue .button": {
        "background-color": "#007DD1"
    },
    ".body_pink .button": {
        "background-color": "#FF00E1"
    },
    ".textbox": {
        "border-radius": "5px",
        "border": "none",
        "border-bottom": "1px solid #808080A0",
        "background-color": "#FFFFFF",
        "color": "#000000",
        "width": "380px",
        "max-width": "380px",
        "height": "50px",
        "font-size": "20px",
        "padding-left": "10px",
        "padding-right": "10px"
    },
    ".textbox:focus": {
        "outline": "none"
    },
    ".button:focus": {
        "box-shadow": "inset 0 0 100px 100px #FFFFFF60",
        "outline": "none"
    },
    ".button:hover": {
        "box-shadow": "inset 0 0 100px 100px #FFFFFF60",
        "outline": "none"
    },
    ".textbox.grey": {
        "background-color": "#F0F0F0"
    },
    ".body_red .textbox.grey": {
        "background-color": "#FFE9E9"
    },
    ".body_orange .textbox.grey": {
        "background-color": "#FFFAEC"
    },
    ".body_yellow .textbox.grey": {
        "background-color": "#FFFF8E"
    },
    ".body_green .textbox.grey": {
        "background-color": "#E8FFE8"
    },
    ".body_blue .textbox.grey": {
        "background-color": "#E5EFFF"
    },
    ".body_pink .textbox.grey": {
        "background-color": "#FFE6FC"
    },
    "a": {
        "color": "inherit",
        "text-decoration": "inherit"
    },
    ".app_icon": {
        "padding": "20px",
        "border-radius": "20px",
        "background-color": "#808080",
        "width": "40px",
        "height": "40px"
    },
    ".app_icon span": {
        "color": "#FFFFFFD0",
        "font-size": "40px"
    },
    ".app_icon img": {
        "height": "40px",
        "width": "40px",
        "filter": "opacity(0.85)"
    },
    ".body_red .app_icon": {
        "background-color": "#FF0000"
    },
    ".body_orange .app_icon": {
        "background-color": "#FFA200"
    },
    ".body_yellow .app_icon": {
        "background-color": "#FFFF00"
    },
    ".body_green .app_icon": {
        "background-color": "#00FF00"
    },
    ".body_blue .app_icon": {
        "background-color": "#007DD1"
    },
    ".body_pink .app_icon": {
        "background-color": "#FF00E1"
    },
